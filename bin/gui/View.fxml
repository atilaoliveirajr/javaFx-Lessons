<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <Label layoutX="162.0" layoutY="86.0" text="Calculate Sum" />
      <TextField fx:id="txtNumber1" layoutX="35.0" layoutY="129.0" promptText="First number" />
      <TextField fx:id="txtNumber2" layoutX="217.0" layoutY="129.0" promptText="Second number" />
      <Text layoutX="196.0" layoutY="146.0" strokeType="OUTSIDE" strokeWidth="0.0" text="+" />
      <Button fx:id="brSum" layoutX="174.0" layoutY="175.0" mnemonicParsing="false" onAction="#onBtSumAction" text="Sum" />
      <Label layoutX="35.0" layoutY="216.0" text="Result =" />
      <Label fx:id="lbResult" layoutX="89.0" layoutY="216.0" />
   </children>
</AnchorPane>
